#include <stdio.h>
#include "rajdeep.h"

int main() {
    int num, result;

    printf("Enter a three-digit integer: ");
    scanf("%d", &num);

    result = isArmstrongNumber(num);

    if (result)
        printf("%d is an Armstrong number.\n", num);
    else
        printf("%d is not an Armstrong number.\n", num);

    return 0;
}